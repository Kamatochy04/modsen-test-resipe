    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@types/gh-pages": "^6",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.5.0",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/react-router-dom": "^5.3.3",
        "@types/styled-components": "^5.1.34",
